{
	"authors": [
		{
			"homepage": "https://mahdiyazdani.one",
			"name": "WordPress Footer Rate Notice"
		}
	],
	"autoload": {
		"classmap": [
			"src"
		]
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"description": "Library to be bundled with WordPress themes and plugins to ask for users to leave a review.",
	"extra": {
		"scripts-description": {
			"lint:wpcbf": "Fix coding standards warnings/errors automatically with PHP Code Beautifier",
			"lint:wpcs": "Analyze code against the WordPress coding standards with PHP_CodeSniffer"
		}
	},
	"homepage": "https://github.com/mahdiyazdani/wp-footer-rate",
	"keywords": [
		"WordPress",
		"Plugin"
	],
	"license": "GPL-3.0",
	"name": "mahdiyazdani/wp-footer-rate",
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "0.7.2",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phpcompatibility/phpcompatibility-wp": "2.1.3",
		"phpunit/phpunit": "9.5.24",
		"squizlabs/php_codesniffer": "3.7.1",
		"wp-coding-standards/wpcs": "2.3.0"
	},
	"scripts": {
		"lint:wpcbf": "phpcbf --extensions=php -p --runtime-set testVersion 7.4-",
		"lint:wpcs": "phpcs --extensions=php -s -p --runtime-set testVersion 7.4-"
	},
	"type": "wordpress-plugin"
}
